@{
    ViewBag.Title = "Xoá nhà cung";
}
<div class="box box-primary">
    <div class="box-body">
        <form>
            <div class="form-group">
                <label>Tên nhà cung cấp:</label>
                <p class="form-control-static form-control">Trần Như Ý</p>
            </div>

            <div class="form-group">
                <label>Tên giao dịch:</label>
                <p class="form-control-static form-control">GD01</p>
            </div>

            <div class="form-group">
                <label>Điện thoại:</label>
                <p class="form-control-static form-control">0123456789</p>
            </div>

            <div class="form-group">
                <label>Email:</label>
                <p class="form-control-static form-control">nhuytran@gmail.com</p>
            </div>

            <div class="form-group">
                <label>Địa chỉ:</label>
                <p class="form-control-static form-control">QĐ</p>
            </div>

            <div class="form-group">
                <label>Tỉnh thành:</label>
                <p class="form-control-static form-control">TP Huế</p>
            </div>

            <div class="form-group text-right">
                <button type="submit" class="btn btn-danger">
                    <i class="fa fa-trash"></i>
                    Xóa dữ liệu
                </button>
                <a href="~/Supplier" class="btn btn-default">
                    Quay lại
                </a>
            </div>

        </form>
    </div>
</div>
